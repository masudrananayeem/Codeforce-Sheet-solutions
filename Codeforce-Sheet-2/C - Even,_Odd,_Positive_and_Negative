#include <stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int num[n];
    for(int i=0;i<n;i++){
        int a;
        scanf("%d",&a);
        num[i]=a;
    }
      
    int evenCount = 0, oddCount = 0, positiveCount = 0, negativeCount = 0;
    
    for (int i = 0; i < n; i++) {
        if (num[i] % 2 == 0) {
            evenCount++;
        } else {
            oddCount++;
        }
        if (num[i] > 0) {
            positiveCount++;
        } else if (num[i] < 0) {
            negativeCount++;
        }
    }
    
    printf("Even: %d\n", evenCount);
    printf("Odd: %d\n", oddCount);
    printf("Positive: %d\n", positiveCount);
    printf("Negative: %d\n", negativeCount);
    
    return 0;
}
